{
  "name": "skuzalabs/laravel-menus",
  "description": "Laravel Menu management",
  "keywords": [
    "laravel",
    "menus",
    "navigation",
    "nav",
    "bootstrap",
    "tailwindcss"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    },
    {
      "name": "Pingpong Labs",
      "email": "pingpong.labs@gmail.com"
    },
    {
      "name": "SkuzaLabs",
      "email": "skuzalabs@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3|^8.0|^8.1|^8.2",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/config": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/view": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "laravelcollective/html": "^5.0|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^6.2",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "SkuzaLabs\\Menus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SkuzaLabs\\Menus\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0-dev"
    },
    "laravel": {
      "providers": [
        "SkuzaLabs\\Menus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "SkuzaLabs\\Menus\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "dev"
}
